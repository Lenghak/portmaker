{
  "name": "@lenghak/portmaker",
  "dependencies": {
    "turbo": "^2.5.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@prettier/plugin-xml": "^3.4.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "syncpack": "14.0.0-alpha.19",
    "ultracite": "^5.1.8"
  },
  "lint-staged": {
    "*.{astro,vue,svelte}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx,json,jsonc,css,scss,md,mdx}": [
      "ultracite format"
    ]
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748",
  "peerDependencies": {
    "typescript": "^5.9.2"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@latest"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "vue-demi"
    ]
  },
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "format": "ultracite format",
    "format:check": "ultracite format --dry-run",
    "format:unsafe": "ultracite format --unsafe",
    "lint": "ultracite lint",
    "lint:check": "ultracite lint --dry-run",
    "type-check": "turbo run type-check",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier --write \"**/*.{astro,vue,svelte,ts,tsx,js,jsx,json,jsonc,css,scss,md,mdx}\""
  }
}
